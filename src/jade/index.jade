doctype html
html
  head
    title Gulp and Jade Playground
    meta(charset="utf-8")
    link(src="/css/main.css", rel="stylesheet")

  body
    include includes/mixin.jade
    
    mixin block
      p#par1 Mixin # [strong Title 1]
      p#par2 Mixin Title 2
      //
        img(src="../img/cute-kitty.jpg", alt="Kitten")
        img(src="../img/kitty.png", alt="Kitten")
      
    + block
    
    
    
    script(src="./js/bundle.js")
